#documents-banner .center-content #breadcrumbs span a { text-decoration: none; font-family: Helvetica light; font-size: 1rem; letter-spacing: .4px; color: var(--black); margin-right: .5em; }
#documents-banner .center-content #breadcrumbs .breadcrumb_last { font-family: Helvetica medium; color: var(--blue2); font-size: 1rem; letter-spacing: .4px; margin-left: .5em; }
