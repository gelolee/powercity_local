:root{
    --white: #FFFFFF;
    --white2: #DEDEDE00;
    --white3: #F2F6F9;
    --white4: #F8F8F8;
    --white5: #CECECE50;
    --white6: #FAFAFA;
    --white7: #FFFDFD;
    --grey : #EEEEEE;
    --grey2: #CECECE;
    --grey3: #393939;
    --grey4: #8E8E8E;
    --grey5: #00000029;
    --grey6: #575757;
    --grey7: #7E7E7E;
    --grey8: #5F5F5FBA;
    --grey9: #00000081;
    --grey10: #666B6C;
    --grey11: #707070; 
    --grey12: #D3D3D369;
    --grey13:#B1ABAB;
    --black: #000000;
    --black2: #171717;
    --blue: #24B4C2;
    --blue2: #10B6C5;
    --blue3: #0FB5C3;
    --fade-blue: #24B4C224;
    --green: #125A61;
    --green2: #0A7680;
    --green3: #1A8F9C;
    --green4: #2D929E;
    --green5: #11B6C5;
    --dark-blue: #1B3246;
}

