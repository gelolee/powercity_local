/* Services */
#services-banner .banner-bg { position: relative; }
#services-banner .banner-bg img { width: 100%; }
#services-banner .banner-bg svg { position: absolute; left: 0; }
#services-banner .service-content { position: absolute; top: 50%; left: 50%; width: 100%; transform: translate(-50%, -60%); }
#services-banner .service-content h2 { font-family: 'Helvetica bold'; font-size: 2.5rem; color: var(--black); letter-spacing: 2px; text-transform: uppercase; line-height: 2.5; }
