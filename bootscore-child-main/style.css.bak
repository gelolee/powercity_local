/* Navbar */
#nav-main { background: var(--blue2); opacity: 1; padding: 0;}
#nav-main .offcanvas-body { justify-content: end; }
#top-bar { background: var(--blue2); border-bottom: 1px solid;}
#top-bar .navbar-logo { padding: 12px 0; display: flex; justify-content: space-between; align-items: center;}
#top-bar .navbar-logo img { width: 50%; }
#top-bar .navbar-logo span { font-family: 'Helvetica light'; font-weight: 600; font-size: 1rem; color: var(--black); margin: 0; line-height: 50px; letter-spacing: .8px; opacity: 1; display: flex; align-items: center; }
#top-bar .navbar-logo span i { margin-right: .4em; }
#top-bar .navbar-logo span .fa-magnifying-glass { margin-left: 2em; }
#nav-main ul { align-items: center;}
#nav-main ul li a { font-family: 'Helvetica medium'; color: var(--black); font-size: 1rem; line-height: 19px; letter-spacing: .8px; text-transform: uppercase; }
#nav-main ul li:nth-child(6) { background: var(--grey); padding: 20px 20px; margin: 0;}
#nav-main ul li { margin-right: 1.5em; }
#top-bar .button { display: inline-block; margin: 4px 2px; background-color: transparent; font-size: 14px; padding-left: 32px; padding-right: 32px; height: 50px; line-height: 50px; text-align: center; color: var(--black); text-decoration: none; cursor: pointer; -moz-user-select: none; -khtml-user-select: none; -webkit-user-select: none; -ms-user-select: none; user-select: none;}
/* #top-bar .button:hover { transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.4s; -o-transition-duration: 0.4s; background-color: white; color: black; } */
#top-bar .search-container { position: relative; display: inline-block; margin: -2px 2px; height: 50px; width: 50px; vertical-align: bottom;}
#top-bar .searchbutton { position: absolute; font-size: 18px; width: 100%; margin: 0; padding: 0; }
#top-bar .search:focus + .searchbutton { transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.4s; -o-transition-duration: 0.4s; background-color: white; color: black }
#top-bar .search { position: absolute; left: 49px; background-color: white; outline: none; border: none; padding: 0; width: 0; height: 100%; z-index: 10; transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.7s; -o-transition-duration: 0.4s; }
#top-bar .search:focus { width: 363px; padding: 0 16px 0 0;}
#top-bar .expandright { left: auto; right: 49px; max-width: 250px;}
#top-bar .expandright:focus { padding: 0 0 0 16px; }
/* #top-bar .button:hover { transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.4s; -o-transition-duration: 0.4s; background-color: white; color: black; } */
.header-actions .button-2 { display: inline-block; margin: 4px 2px; background-color: transparent; font-size: 14px; padding-left: 32px; padding-right: 32px; height: 50px; line-height: 50px; text-align: center; color: var(--black); text-decoration: none; cursor: pointer; -moz-user-select: none; -khtml-user-select: none; -webkit-user-select: none; -ms-user-select: none; user-select: none;}
.header-actions .search-container-2 { position: relative; display: inline-block; margin: -2px 2px; height: 50px; width: 50px; vertical-align: bottom;}
.header-actions .searchbutton-2 { position: absolute; font-size: 18px; width: 100%; margin: 0; padding: 0; }
.header-actions .search-2:focus + .searchbutton-2 { transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.4s; -o-transition-duration: 0.4s; color: black }
.header-actions .search-2 { position: absolute; left: 49px; background-color: white; outline: none; border: none; padding: 0; width: 0; height: 100%; z-index: 10; transition-duration: 0.4s; -moz-transition-duration: 0.4s; -webkit-transition-duration: 0.7s; -o-transition-duration: 0.4s; }
