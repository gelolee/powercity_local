/* Footer */
.bootscore-footer, .bootscore-info { background: var(--white3); }
.bootscore-footer h2 { font-family: 'Montserrat bold italic'; font-size: 1.25rem; margin-bottom: 1.5em;}
.bootscore-footer ul li a { text-decoration: none; font-family: 'Helvetica light'; color: var(--grey3); line-height: 22px; font-size: 1rem; font-weight: 600; letter-spacing: .4px; }
.bootscore-footer ul li a:hover { color: var(--blue3); }
.bootscore-footer h3 { font-family: 'Helvetica light'; color: var(--grey3); line-height: 22px; font-size: 1rem; max-width: 90%; font-weight: 600; letter-spacing: .4px; }
.bootscore-footer span:hover { color: var(--blue3); cursor:default; }
.bootscore-footer span:hover h3{ color: var(--blue3); cursor:default; }
.bootscore-footer .footer-three > .footer-content{ display: grid; gap: 21px; }
.bootscore-footer .footer-three .footer-content > span { display: flex; align-items: baseline;}
.bootscore-footer .footer-three .footer-content > span i { font-size: 1rem; margin-right: 10px; margin-top: 5px; }
.bootscore-footer ul { display: grid; gap: 21px; }
.bootscore-info .bootscore-copyright { font-family: 'Helvetica light'; font-size: 1rem; color: var(--grey3); font-weight: 600; padding-bottom: 1em;}
.bootscore-info hr { margin: 0; margin-bottom: 1em; }
.color-white .footer-1 .navbar-brand img { max-width: 80%; }
.color-white .row .custom-footer { padding-left: 30px; }
.color-white .row .col-md-6.col-lg-3:nth-last-child(1){ padding-left: 30px; }

/* Second Footer */
.second-footer { position: relative; }
.second-footer .footer_widget img { width: 100%; height: 401px; }
.second-footer .footer_widget .custom-html-widget { position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); width: 100%; text-align: center; }
.second-footer .footer_widget .custom-html-widget h2 { font-family: 'Helvetica bold'; font-size: 2.5rem; color: var(--white); margin-bottom: 1.7em;}
.second-footer .footer_widget { margin-bottom: 0!important; }
.second-footer .footer_widget .custom-html-widget a { background: var(--grey); color: var(--black); text-align: center; text-decoration: none; font-size: 1.125rem; font-family: 'Helvetica bold'; letter-spacing: .9px; padding: 20px 31px; box-shadow: 5px 5px 1px 1px var(--green3); text-transform: uppercase; }
.second-footer .footer_widget .custom-html-widget a:hover { background: var(--green3); box-shadow: 5px 5px 1px 1px var(--grey); color: var(--white); }

/* Homepage */
#homepage-banner { position: relative; }
#homepage-banner .image-banner img { position: relative; width: 100%; }
/* #homepage-banner .image-banner{ display: flex; flex-direction: row-reverse; } */
/* #homepage-banner .image-banner img:first-child {  } */
/* #homepage-banner .image-banner img:nth-child(2) { float: left; position: absolute; z-index: -1; } */
/* #homepage-banner .image-banner img:last-child { float: left; } */
#homepage-banner .section1-content { position: absolute; top: 40%; left: 50%; transform: translate(-50%, -50%); width: 100%; }
#homepage-banner .section1-content h2 { font-family: 'Helvetica bold'; font-size: 2.5rem; color: var(--black); letter-spacing: 2px; line-height: 49px; max-width: 370px; text-transform: uppercase; }
#homepage-banner .section1-content p { font-family: 'Helvetica light'; font-size: 1rem; color: var(--black); letter-spacing: .4px; line-height: 30px; max-width: 365px; }
#section-two { padding-block: 170px 80px; margin-top: -165px; z-index: 1; position: relative; background: rgb(36,180,194);background: linear-gradient(180deg, rgba(36,180,194,1) 0%, rgba(35,177,191,1) 24%, rgba(36,180,194,1) 24%, rgba(20,99,107,1) 77%, rgba(18,90,97,1) 100%, rgba(0,212,255,1) 100%); clip-path: polygon(0 24%, 150% 0, 100% 100%, 0 88%); opacity: 1; }
#section-two .content p { font-family: 'Helvetica bold'; font-size: 2.5rem; letter-spacing: .4px; line-height: 50px; text-align: center; margin-bottom: 1em; color: var(--white); }
#section-two .number-count { display: flex; justify-content: space-evenly;}
#section-two .number-count .numbers p { font-family: 'Helvetica bold'; font-size: 4.313rem; margin: 0; letter-spacing: .1px; color: var(--white);}
#section-two .number-count .numbers p i { font-size: x-large; position: absolute; translate: -10% 60%; color: var(--white);}
#section-two .number-count .numbers-2 p { font-size: 1.125rem; font-family: 'Helvetica light';  translate: 0% -80%; letter-spacing: -0.03px; }
#section-three { position: relative;}
#section-three .img-products { margin-top: -70px; }
#section-three .img-products .row { padding-block: 60px; }
#section-three .img-products > img { position: absolute; left: 0; right: 0; width: 100%; height: 100%; object-fit: cover; }
#section-three .row { align-items: center; position: relative; }
#section-four { margin-top: 4em;}
#section-four .machine-product { padding: 2em 0; }
#section-four .machine-product .back-shadow { position: absolute; left: 50%; translate: -50%; top: 0; width: 100%; text-align: center; z-index: -1; }
#section-four .machine-product .back-shadow h3 { font-size: 4rem; font-family: Helvetica bold; color: var(--grey); opacity: 50%; letter-spacing: 1px; text-transform: uppercase; }
#section-four .machine-product .right { display: grid; place-content: center; }
#section-four .machine-product .right p { font-family: 'Helvetica bold'; font-size: 1.5rem; color: var(--grey3); text-transform: uppercase; margin-bottom: 1.5em;}
#section-four .machine-product .right p:nth-child(n+2) { font-family: 'Helvetica italic'; font-size: 1rem; color: var(--grey3); margin: 0; margin-bottom: 1em; text-transform: capitalize;}
#section-four .machine-product .right ul li { font-family: 'Helvetica medium'; font-size: 1.375rem; color: var(--grey3); }
#section-four .machine-product .right a { background: var(--grey); color: var(--black); text-align: center; text-decoration: none; font-size: 1.125rem; font-family: 'Helvetica bold'; letter-spacing: .9px; padding: 20px 31px; box-shadow: 5px 5px 1px 1px var(--green3); text-transform: uppercase; }
#section-four .machine-product .right a:hover { background: var(--green3); box-shadow: 5px 5px 1px 1px var(--grey); color: var(--white); }
#section-four .machine-product h2 { text-align: center; font-family: 'Helvetica bold'; font-size: 3rem; color: var(--black); margin-top: .1em; margin-bottom: 1em;}
/* #section-four .owl-nav .owl-prev { position: absolute; font-size: 3rem; top: calc(50% - 25px); left: -50px; }
#section-four .owl-nav .owl-next { position: absolute; font-size: 3rem; top: calc(50% - 25px); right: -50px; } */
#section-four .owl-carousel.products { padding: 0 50px; }
#section-four .owl-nav button { position: absolute; top: 50%; transform: translateY(-50%); font-size: 1.125rem; }
#section-four .owl-nav .owl-prev { left: 0px; }
#section-four .owl-nav .owl-next { right: 0px; }
#section-four .owl-nav i { background: var(--grey); color: var(--grey6); border-radius: 50%; padding: 17px; }
#section-four .owl-nav i:hover { background: var(--blue3); color: var(--white); }
#section-five .slider { position: relative; background: transparent linear-gradient(89deg, #CECECE50 0%, #DEDEDE00 100%) 0% 0% no-repeat padding-box; padding-bottom: 12px; }
#section-five .slider .back-shadow { position: absolute; left: 50%; translate: -50%; width: 100%; text-align: center; z-index: -1; margin-top: 1.5em;}
#section-five .slider .back-shadow h3 { font-family: Helvetica bold; font-size: 6rem; letter-spacing: 1.6px; color: var(--grey); opacity: 50%; text-transform: uppercase;}
#section-five .slider h2 { text-align: center; font: normal normal bold 3.5rem/97px 'Helvetica bold'; color: var(--black); letter-spacing: .8px; padding-top: 1.1em; padding-bottom: 1em; text-transform: uppercase;}
#section-five .list-group-item { border: none; }
#section-five .card .body ul li { font-family: 'Helvetica light'; font-size: 1.125rem; padding: 1.2em 1.5em; cursor: pointer; color: var(--grey6); background: var(--white6); display: flex; justify-content: space-between; align-items: center; letter-spacing: 1.5px;}
#section-five .card .body ul li i { background: var(--grey); padding: 16px; padding-bottom: 14px; border-radius: 50%; }
#section-five .card .body ul li:hover { background: var(--white); color: var(--black); font-weight: 600;}
#section-five .card .body ul li:hover i { background: var(--blue3); color: var(--white); } 
#section-five .card .body ul .list-group-item.active { background-color: var(--white); color: var(--black); font-weight: 600; }
#section-five .card .body ul .list-group-item.active i { background-color: var(--blue3); color: var(--white); } 
#section-five .card img { height: 100%; width: auto; aspect-ratio: 12/6.3; object-fit: cover; }
#section-five .card { border-radius: 0; background: transparent; border: none; }
#clients-review { padding-block: 5em; }
#clients-review .card { border: none; background: var(--white4); box-shadow: 0px 3px 6px #00000029; opacity: 1; }
#clients-review .card p { margin: 0; padding: 30px; font-family: Helvetica light; font-size: 1rem; color: var(--black); letter-spacing: .32px; }
#clients-review .card .card-body h2 { font-family: Helvetica medium; font-size: 1.125rem; color: var(--green2); }
#clients-review .card .card-body h3 { font-family: Helvetica light; font-weight: 100; color: var(--black); font-size: .875rem; }
#clients-review .card .card-body span { font-family: Helvetica medium italic; font-size: .875rem; color: var(--green2); }
#clients-review .card .card-body { padding-inline: 30px; }
#clients-review .client-review .owl-stage-outer { padding-block: .5em; }
#clients-review .client-review .owl-nav { display: block; text-align: center; margin-top:1em; }
#clients-review .client-review .owl-dots { display: none; }
#clients-review .client-review .owl-nav .owl-next, #clients-review .client-review .owl-nav .owl-prev { font-size: 1.125rem; margin-inline: 15px; }
#clients-review .client-review .owl-nav i { color: var(--grey6); background: var(--grey); padding: 17px; border-radius: 50%; }
#clients-review .client-review .owl-nav i:hover { background: var(--blue3); color: var(--white); }
#clients-review .left-side h2 { text-transform: uppercase; font-family: Helvetica bold; font-size: 2.5rem; letter-spacing: .4px; color: var(--black); margin-top: 1.1em; margin-left: 65px; }
#clients-review .left-side p { font-family: Helvetica light; color: var(--black); font-size: 1rem; letter-spacing: .4px; max-width: 340px; line-height: 30px; }
#clients-review .left-side { position: relative; }
#clients-review .left-side .back-shadow { position: absolute; left: 40%; translate: -50%; width: 100%; text-align: center; z-index: -1;}
#clients-review .left-side .back-shadow h3 { font-family: Helvetica bold; font-size: 5.063rem; color: var(--grey); text-transform: uppercase; opacity: 50%; }

